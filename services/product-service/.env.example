# Application Configuration
NODE_ENV=development
PORT=3000

# Dapr Configuration
DAPR_HOST=127.0.0.1
DAPR_HTTP_PORT=3500
DAPR_GRPC_PORT=50001

# Dapr Pub/Sub Configuration
PUBSUB_NAME=product-pubsub
PRODUCT_TOPIC=product-events

# AWS Configuration (for production with IRSA)
AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=  # Not needed with IRSA in EKS
# AWS_SECRET_ACCESS_KEY=  # Not needed with IRSA in EKS

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json

# API Configuration
API_PREFIX=/api
SWAGGER_ENABLED=true

# Service Configuration
SERVICE_NAME=product-service
SERVICE_VERSION=2.0.0
